{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
      }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
    },
    "plugins": [
        "react",
    ],
    "extends": [
        "airbnb",
    ],
    "rules": {
        "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
        "no-underscore-dangle": 0,
    }
}
